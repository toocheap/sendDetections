# Example environment variables for sendDetections
# Copy this file to .env and modify as needed

# API settings
RF_API_TOKEN=your_token_here

# Optional: Override default API URL
RF_API_URL=https://api.recordedfuture.com/collective-insights/detections

# API options
RF_API_OPTIONS_DEBUG=false
RF_API_OPTIONS_SUMMARY=true

# Processing settings
RF_MAX_CONCURRENT=5
RF_BATCH_SIZE=100
RF_MAX_RETRIES=3